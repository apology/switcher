{
	"name": "Switcher",
	"version": "0.1",
	"manifest_version": 2,
	"description": "Quickswitcher for Chrome",
	"icons": {
		"128": "icon.png"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"commands": {
		"open": {
			"suggested_key": {
				"default": "Ctrl+K",
				"mac": "Command+K"
			},
			"description": "Open Switcher"
		}
	},
	"permissions": [
		"tabs",
		"system.display"
	]
}
